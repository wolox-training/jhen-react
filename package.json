{
  "name": "react-training-express",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@hookform/resolvers": "^1.3.8",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "apisauce": "^1.1.2",
    "history": "^4.10.1",
    "i18next": "^19.0.1",
    "node-sass": "^4.14.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.15.4",
    "react-hot-toast": "^2.2.0",
    "react-i18next": "^11.2.5",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "^4.0.1",
    "react-spinkit": "^3.0.0",
    "typescript": "^4.1.3",
    "web-vitals": "^0.2.4",
    "wolox-equalizer": "0.0.3",
    "yup": "^0.32.11"
  },
  "scripts": {
    "start": "rescripts start",
    "start-env": "node ./scripts/start.js",
    "build": "node ./scripts/build.js",
    "test": "rescripts test --env=jsdom",
    "eject": "./node_modules/react-scripts/bin/react-scripts.js eject",
    "lint": "./node_modules/eslint/bin/eslint.js src --ext .js --ext .ts --ext .tsx",
    "lint-fix": "npm run lint -- --fix && ./node_modules/stylelint/bin/stylelint.js '**/*.scss' --fix",
    "lint-scss": "./node_modules/stylelint/bin/stylelint.js '**/*.scss'",
    "lint-diff": "git diff --name-only --cached --relative --diff-filter=ACM | grep -e \\.js$ -e \\.tsx$ -e \\.ts$ | xargs eslint",
    "coverage": "npm run test -- --coverage --watchAll=false",
    "coverage-diff": "npm run test coverage -- --changedSince=development"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@rescripts/cli": "0.0.14",
    "@testing-library/react-hooks": "^3.7.0",
    "@types/react-router": "^5.1.11",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-spinkit": "^3.0.6",
    "@types/webpack-env": "^1.16.0",
    "@typescript-eslint/parser": "^4.14.0",
    "@wolox/eslint-config-typescript": "^1.1.3",
    "aws-deploy-script-fe": "^1.0.8",
    "env-cmd": "^10.1.0",
    "eslint-config-wolox": "^3.0.2",
    "eslint-config-wolox-react": "^2.1.2",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^4.3.8",
    "minimist": "^1.2.0",
    "mutationobserver-shim": "^0.3.7",
    "postcss-html": "^0.36.0",
    "postcss-syntax": "^0.36.2",
    "prettier": "^1.19.1",
    "prettier-eslint": "^8.8.2",
    "prettier-eslint-cli": "^5.0.0",
    "prettier-stylelint": "^0.4.2",
    "react-test-renderer": "^17.0.1",
    "stylelint": "^9.10.1",
    "stylelint-config-wolox": "^1.0.7",
    "stylelint-no-indistinguishable-colors": "^1.2.1",
    "stylelint-scss": "^3.5.4"
  },
  "title": "react-training-express",
  "jest": {
    "moduleNameMapper": {
      "^~(.*)/(.*)$": "<rootDir>/src/$1/$2"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    }
  },
  "repository": {
    "type": "git"
  },
  "engines": {
    "node": ">= 10.15.3",
    "npm": ">= 6.9.0"
  }
}
